pipeline {
  agent any 

  stages {
      stage('checkout'){
          steps {
              echo 'checkout app'
          }
      }
    stage('build'){
          steps {
              echo 'build app'
          }
      }
    stage('test'){
          steps {
                echo 'test app'
          }
      }
    stage('Build Docker Image'){
          steps {
                echo 'build docker image'
          }
      }
    stage('deploy'){
          steps {
                echo 'deploy app'
          }
      }

  }


  
}
